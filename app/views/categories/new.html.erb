# app/views/categories/new.html.erb

<h1>New Category</h1>
<%= form_for @category do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :icon %>
    <%= f.text_field :icon %>
  </div>
  <%= f.submit %>
<% end %>
<%= link_to "Back to categories", categories_path %>
